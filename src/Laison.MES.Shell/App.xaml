<ssh:LapisApp
    x:Class="Laison.MES.Shell.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:langs="clr-namespace:Laison.MES.UI.Langs;assembly=Laison.MES.UI"
    xmlns:langs2="clr-namespace:Laison.Lapis.Shared.UI.Langs;assembly=Laison.Lapis.Shared.UI"
    xmlns:local="clr-namespace:Laison.MES.Shell"
    xmlns:ssh="clr-namespace:Laison.Lapis.Shared.Shell;assembly=Laison.Lapis.Shared.Shell">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Laison.Lapis.Shared.Shell;component/Resources/ShellResource.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <!--  多语言设计  -->
            <langs:LangProvider x:Key="MESLangs" />
            <langs2:LangProvider x:Key="SharedLangs" />
        </ResourceDictionary>
    </Application.Resources>
</ssh:LapisApp>
